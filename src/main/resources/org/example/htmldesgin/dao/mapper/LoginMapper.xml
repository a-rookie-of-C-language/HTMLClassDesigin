<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0.4//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.htmldesgin.dao.mapper.LoginMapper">
    <select id="login" resultType="org.example.htmldesgin.utils.User">
        select * from User.user where email = #{email} and password = #{password}
    </select>
</mapper>