<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.htmldesgin.dao.mapper.TrainerWorkingHoursMapper">

    <delete id="deleteTrainerWorkingHours" parameterType="java.lang.String">
        delete from User.trainerWorkingHours where username in
        <foreach collection="list" item="username" open="(" close=")" separator=",">
            #{username}
        </foreach>
    </delete>
</mapper>